@model MyFirstWebApp.Models.CashierViewModel
 
<div class="modal-content">
    <div class="modal-header">
        <h4>Кассир</h4>
        <button class="close" data-dismiss="modal" area-hidden="true">X</button>
        
    </div>
    <div class="modal-body">
        <dl class="dl-horizontal">
            <dt>Имя кассира:</dt>
            <dd>@Html.DisplayFor(model => model.Cashier.CashierName)</dd>
 
            <dt>Возраст:</dt>
            <dd>@Html.DisplayFor(model => model.Cashier.Age)</dd>
 
            <dt>Магазин:</dt>
            <dd>@Html.DisplayFor(model => model.Cashier.ShopModelId)</dd>
        </dl>
    </div>
</div>